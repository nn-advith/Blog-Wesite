<%- include('partials/header'); -%>
<h1>Compose</h1>
<br /><br />
<form class="" action="/compose" method="post">
  <div class="form-group">
    <label for="titletext">Title</label>
    <input type="text" class="form-control textfield" id="titletext" name="titletext">
  </div>
  <div class="form-group">
    <label for="maincontent">Post</label>
    <textarea class="form-control textfield" id="maincontent" name="maincontent" rows="15"></textarea>
  </div>
  <button type="submit" class="btn">Publish</button>

</form>

<%- include('partials/footer'); -%>
